{
  "block:center block": {
    "description": "#+BEGIN_CENTER block",
    "prefix": "block_center",
    "body": ["#+BEGIN_CENTER", "$0", "#+END_CENTER"]
  },
  "block:comment": {
    "description": "#+BEGIN_COMMENT block",
    "prefix": "block_comment",
    "body": ["#+BEGIN_COMMENT", "$0", "#+END_COMMENT"]
  },
  "block:example": {
    "description": "#+BEGIN_EXAMPLE block",
    "prefix": "block_example",
    "body": ["#+BEGIN_EXAMPLE", "$0", "#+END_EXAMPLE"]
  },
  "block:src": {
    "description": "#+BEGIN_SRC ... block",
    "prefix": "block_code_",
    "body": ["#+BEGIN_SRC ${1:lang}", "$0", "#+END_SRC"]
  },
  "block:verse": {
    "description": "verse",
    "prefix": "block_verse",
    "body": ["#+BEGIN_VERSE", "$0", "#+END_VERSE"]
  },
  "block:export ascii": {
    "description": "#+BEGIN_EXPORT ascii",
    "prefix": "block_ascii",
    "body": ["#+BEGIN_EXPORT ascii", "$0", "#+END_EXPORT"]
  },
  "block:export html": {
    "description": "#+BEGIN_EXPORT html block",
    "prefix": "block_html",
    "body": ["#+BEGIN_EXPORT html", "$0", "#+END_EXPORT"]
  },
  "block:export latex": {
    "description": "#+BEGIN_EXPORT latex block",
    "prefix": "block_latex",
    "body": ["#+BEGIN_EXPORT latex", "$0", "#+END_EXPORT"]
  },
  "block:quote": {
    "description": "#+BEGIN_QUOTE block",
    "prefix": "block_quote",
    "body": ["#+BEGIN_QUOTE", "$0", "#+END_QUOTE"]
  },
  "block:blog": {
    "description": "blog",
    "prefix": "block_blog",
    "body": [
      "#+STARTUP: showall indent",
      "#+STARTUP: hidestars",
      "#+BEGIN_HTML",
      "---",
      "layout: default",
      "title: ${1:title}",
      "excerpt: ${2:excerpt}",
      "---",
      "$0"
    ]
  },
  "block:dot": {
    "description": "dot",
    "prefix": "block_dot",
    "body": [
      "#+BEGIN_SRC dot :file ${1:file} :cmdline -T${2:pdf} :exports none :results silent",
      "$0",
      "#+END_SRC",
      "",
      "[[file:$1]]"
    ]
  },
  "block:elisp": {
    "description": "elisp",
    "prefix": "block_elisp",
    "body": ["#+BEGIN_SRC emacs-lisp :tangle yes", "$0", "#+END_SRC"]
  },
  "block:entry": {
    "description": "entry",
    "prefix": "block_entry",
    "body": [
      "#+BEGIN_HTML",
      "---",
      "layout: ${1:default}",
      "title: ${2:title}",
      "---",
      "#+END_HTML",
      "$0"
    ]
  },
  "block:export": {
    "description": "export",
    "prefix": "block_export",
    "body": ["#+BEGIN_EXPORT ${1:type}", "$0", "#+END_EXPORT"]
  },
  "title:figure": {
    "description": "figure",
    "prefix": "title_figure",
    "body": [
      "#+CAPTION: ${1:caption}",
      "#+ATTR_LATEX: ${2:scale=0.75}",
      "#+LABEL: fig:${3:label}"
    ]
  },
  "title:author": {
    "description": "org-file header",
    "prefix": "title_head_author",
    "body": [
      "#+TITLE:     ${1:Untitled Document}",
      "#+AUTHOR:    ${2:Author}",
      "#+DATE:      ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}"
    ]
  },
  "latex:": {
    "description": "latex",
    "prefix": "latex_",
    "body": ["#+BEGIN_LATEX", "$0", "#+END_LATEX"]
  },
  "latex:block": {
    "description": "matrix",
    "prefix": "latex_block",
    "body": [
      "\\left \\(",
      "\t\\begin{array}{${1:ccc}}",
      "\t\t${2:v1 & v2} \\\\\\",
      "\t\t$0",
      "\t\\end{array}",
      "\\right \\)"
    ]
  },
  "todo": {
    "description": "TODO item",
    "prefix": "todo_",
    "body": ["TODO ${1:task description}"]
  },
  "todo:checklist:try:try_attempt": {
    "description": "Snippet nested step task",
    "prefix": "todo_try_attempt",
    "body": [
      "- try-attempt: ${0:(what todo)}",
      "  description: (penjelasan lanjutan)",
      "  link: ...",
      "  result: [ONGOING|NOTHING|POSTPONE]"
    ]
  },
  "todo:checklist:create": {
    "prefix": "todo_check",
    "body": ["- [ ] $0"],
    "description": "Create a nested TODO checklist item"
  },
  "todo:checklist:nested": {
    "prefix": "todo_check_nested",
    "body": ["- [ ] $1", "\t\t- [ ] $0"],
    "description": "TODO checklist item with nested checklist items"
  },
  "text:lits": {
    "prefix": "text_list",
    "body": ["- $0"],
    "description": "Create a nested list item"
  },
  "text:embedded": {
    "description": "embedded",
    "prefix": "text_embed",
    "body": ["src_${1:lang}${2:[${3:where}]}{${4:code}}"]
  },
  "text:image": {
    "description": "img",
    "prefix": "text_img",
    "body": [
      "#+ATTR_HTML: :alt $2 :align ${3:left} :class img",
      "[[${1:src}]${4:[${5:title}]}]",
      "$0"
    ]
  },
  "text:bold": {
    "description": "bold",
    "prefix": "text_bold",
    "body": ["*$0*"]
  },
  "text:italic": {
    "description": "italic words",
    "prefix": "text_italic",
    "body": ["/$0/"]
  },
  "text:link": {
    "description": "link",
    "prefix": "text_link",
    "body": ["[[${1:URL}][${2:names}]]$0"]
  },
  "text:underline": {
    "description": "underline words",
    "prefix": "text_underline",
    "body": ["_$0_"]
  },
  "text:schedule": {
    "description": "underline words",
    "prefix": "text_schedule",
    "body": ["SCHEDULED:"]
  }
}
