{
  "addEventListener": {
    "prefix": "document_addEventListener",
    "body": "${1:document}.addEventListener('${2:event}', ${3:ev} => {\n\t${0}\n})",
    "description": "document.addEventListener('event', ev} => {\n\n})"
  },
  "removeEventListener": {
    "prefix": "document_removeEventListener",
    "body": "${1:document}.removeEventListener('${2:event}', ${3:listener})",
    "description": "document.removeEventListener('event', listener)"
  },
  "getElementById": {
    "prefix": "document_getElementById",
    "body": "${1:document}.getElementById('${2:id}')",
    "description": "document.getElementById('id')"
  },
  "getElementsByClassName": {
    "prefix": "document_array_getElementByClassName",
    "body": "Array.from(${1:document}.getElementsByClassName('${2:class}'))",
    "description": "Array.from(document.getElementsByClassName('class'))"
  },
  "getElementsByTagName": {
    "prefix": "document_array_getElementByTagName",
    "body": "Array.from(${1:document}.getElementsByTagName('${2:tag}'))",
    "description": "Array.from(document.getElementsByTagName('tag'))"
  },
  "querySelector": {
    "prefix": "document_querySelector",
    "body": "${1:document}.querySelector('${2:selector}')",
    "description": "document.querySelector('selector')"
  },
  "querySelectorAll": {
    "prefix": "document_array_querySelectorAll",
    "body": "Array.from(${1:document}.querySelectorAll('${2:selector}'))",
    "description": "Array.from(document.querySelectorAll('selector'))"
  },
  "createDocumentFragment": {
    "prefix": "document_createDocumentFragment",
    "body": "${1:document}.createDocumentFragment(${2:elem})",
    "description": "document.createDocumentFragment(elem)"
  },
  "createElement": {
    "prefix": "document_createElement",
    "body": "${1:document}.createElement(${2:elem})",
    "description": "document.createElement(elem)"
  },
  "classList.add": {
    "prefix": "document_class_add",
    "body": "${1:el}.classList.add('${2:class}')",
    "description": "el.classList.add('class')"
  },
  "classList.remove": {
    "prefix": "document_class_remove",
    "body": "${1:el}.classList.remove('${2:class}')",
    "description": "el.classList.remove('class')"
  },
  "classList.toggle": {
    "prefix": "document_class_toggle",
    "body": "${1:el}.classList.toggle('${2:class}')",
    "description": "el.classList.toggle('class')"
  },
  "getAttribute": {
    "prefix": "document_class_getAttribute",
    "body": "${1:el}.getAttribute('${2:attr}')",
    "description": "el.getAttribute('attr')"
  },
  "removeAttribute": {
    "prefix": "document_removeAttribute",
    "body": "${1:el}.removeAttribute('${2:attr}')",
    "description": "el.removeAttribute('attr')"
  },
  "setAttribute": {
    "prefix": "document_setAttribute",
    "body": "${1:el}.setAttribute('${2:attr}', ${3:value})",
    "description": "el.setAttribute('attr', value)"
  },
  "appendChild": {
    "prefix": "document_appendChild",
    "body": "${1:el}.appendChild(${2:elem})",
    "description": "el.appendChild(elem)"
  },
  "removeChild": {
    "prefix": "document_removeChild",
    "body": "${1:el}.removeChild(${2:elem})",
    "description": "el.removeChild(elem)"
  },
  "event handler": {
    "prefix": "on_",
    "body": "${1:emitter}.on('${2:event}', (${3:arguments}) => {\n\t${0}\n})",
    "description": "emitter.on('event', (arguments) => {\n\n})"
  },
  "dom event cancel default and propagation": {
    "prefix": "prevent_",
    "body": "ev.preventDefault()\nev.stopPropagation()\nreturn false",
    "description": "ev.preventDefault()\nev.stopPropagation()\nreturn false"
  }
}
